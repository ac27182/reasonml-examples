{
  "name": "faceless-server",
  "version": "0.1.0",
  "bsc-flags": ["-bs-no-version-header", "-bs-super-errors"],
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    },
    {
      "dir": "__tests__",
      "subdirs": true,
      "type": "dev"
    }
  ],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "bs-dependencies": [
    "bs-bastet",
    "relude",
    "bs-express",
    "bs-fetch",
    "faceless-core",
    "@glennsl/bs-json"
  ],
  "bs-dev-dependencies": ["@glennsl/bs-jest"],
  "warnings": {
    "error": "+101"
  },
  "refmt": 3
}
